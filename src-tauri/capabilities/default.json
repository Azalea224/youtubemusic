{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capability for app windows",
  "windows": ["main", "settings"],
  "local": true,
  "remote": {
    "urls": [
      "https://music.youtube.com/*",
      "http://localhost:1420/*",
      "http://127.0.0.1:1420/*"
    ]
  },
  "permissions": [
    "core:default",
    "opener:default",
    "store:default",
    "global-shortcut:default",
    "allow-app-commands"
  ]
}
